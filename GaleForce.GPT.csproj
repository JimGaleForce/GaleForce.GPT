<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<PackageId>GaleForce.GPT</PackageId>
		<Authors>GaleForce</Authors>
		<Copyright>2023 (c) GaleForce Software</Copyright>
		<PackageProjectUrl>https://github.com/JimGaleForce/GaleForce.GPT</PackageProjectUrl>
		<RepositoryUrl>https://github.com/JimGaleForce/GaleForce.GPT</RepositoryUrl>
		<Description>Provides basic GPT capabilities.</Description>
		<Version>1.0.0.1</Version>
		<AssemblyVersion>1.0.0.1</AssemblyVersion>
		<FileVersion>1.0.0.1</FileVersion>
		<PackageReleaseNotes></PackageReleaseNotes>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
	</PropertyGroup>


	<ItemGroup>
		<Compile Remove="TestGaleForce.GPT\**" />
		<EmbeddedResource Remove="TestGaleForce.GPT\**" />
		<None Remove="TestGaleForce.GPT\**" />
		<None Include="LICENSE">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>


	<ItemGroup>
	  <PackageReference Include="GaleForce.OpenAILib" Version="2.0.1" />
	  <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
	</ItemGroup>

</Project>
